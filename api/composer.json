{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "adyen/php-api-library": "^7.1",
        "beyondcode/laravel-websockets": "^1.3",
        "doctrine/dbal": "^2.7",
        "fideloper/proxy": "^4.0",
        "google/apiclient": "^2.0",
        "intervention/image": "dev-master",
        "laravel/framework": "5.7.0.*",
        "laravel/socialite": "~2.0",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "luigel/laravel-paymongo": "1.1.1",
        "paypal/rest-api-sdk-php": "*",
        "pusher/pusher-php-server": "^4.1",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "stripe/stripe-php": "^7.45",
        "tymon/jwt-auth": "0.5.*",
        "werneckbh/laravel-qr-code": "^0.1.3"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "vendor/google/apiclient/src/Google"
        ],
        "psr-4": {
            "App\\": "app/",
            "Increment\\Account\\": "packages/increment/account/src/",
            "Increment\\Imarket\\Merchant\\": "packages/increment/imarket/merchant/src/",
            "Increment\\Imarket\\Product\\": "packages/increment/imarket/product/src/",
            "Increment\\Imarket\\Location\\": "packages/increment/imarket/location/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.2"
        }
    }
}
